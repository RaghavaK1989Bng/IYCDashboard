@model IYCDashboard.Models.DB.State

@{
    ViewBag.Title = "State Details";
    Layout = "~/Views/Shared/_sharedLayout.cshtml";
}

<div>
    <br />
    <h4>State Details</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.LabelFor(model => model.StateName, new { @class = "col-sm-2 control-label" })
        </dt>

        <dd>
            @Html.DisplayFor(model => model.StateName)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Edit", "Edit", new { id = Model.StateID }, new { @class = "btn btn-link" }) |
    @Html.ActionLink("Back to List", "Index",null, new { @class = "btn btn-link" })
</p>
